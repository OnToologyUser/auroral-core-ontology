#################################################################
#    Classes
#################################################################

###  http://lexvo.org/ontology#Language
<http://lexvo.org/ontology#Language> rdf:type owl:Class ;
                                     rdfs:label "Language" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
geo:SpatialThing rdf:type owl:Class ;
                 rdfs:label "Spatial Thing" .


###  http://www.w3.org/ns/sosa/Actuator
<http://www.w3.org/ns/sosa/Actuator> rdf:type owl:Class ;
                                     rdfs:label "Actuator" .


###  http://www.w3.org/ns/sosa/Sensor
<http://www.w3.org/ns/sosa/Sensor> rdf:type owl:Class ;
                                   rdfs:label "Sensor" .


###  http://www.w3.org/ns/ssn/Property
<http://www.w3.org/ns/ssn/Property> rdf:type owl:Class ;
                                    rdfs:label "Property" .


###  http://www.wurvoc.org/vocabularies/om-2/Unit_of_measure
<http://www.wurvoc.org/vocabularies/om-2/Unit_of_measure> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Image
foaf:Image rdf:type owl:Class ;
           rdfs:label "Image" .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:label "Person" .


###  https://auroral.iot.linkeddata.es/def/core#Actuator
core:Actuator rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/sosa/Actuator> ,
                              core:Device ;
              rdfs:label "Actuator" .


###  https://auroral.iot.linkeddata.es/def/core#Audit
core:Audit rdf:type owl:Class ;
           rdfs:label "Audit" .


###  https://auroral.iot.linkeddata.es/def/core#Device
core:Device rdf:type owl:Class ;
            rdfs:subClassOf core:PhysicalThing ,
                            saref:Device ;
            rdfs:label "Device" .


###  https://auroral.iot.linkeddata.es/def/core#Item
core:Item rdf:type owl:Class ;
          rdfs:subClassOf wot:Thing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasStatus ;
                            owl:someValuesFrom core:ItemStatus
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasCreationDate ;
                            owl:allValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasName ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasUpdateDate ;
                            owl:allValuesFrom xsd:dateTime
                          ] ;
          rdfs:label "Item" .


###  https://auroral.iot.linkeddata.es/def/core#ItemStatus
core:ItemStatus rdf:type owl:Class ;
                rdfs:subClassOf core:Status ;
                rdfs:label "Item status" .


###  https://auroral.iot.linkeddata.es/def/core#LuminusFlux
core:LuminusFlux rdf:type owl:Class ;
                 rdfs:label "Luminus Flux" .


###  https://auroral.iot.linkeddata.es/def/core#Node
core:Node rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty core:containsItem ;
                            owl:allValuesFrom core:Item
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasStatus ;
                            owl:allValuesFrom core:NodeStatus
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasCreationDate ;
                            owl:someValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasID ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasKey ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasLocation ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasName ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasToken ;
                            owl:someValuesFrom xsd:string
                          ] ;
          rdfs:label "Node" .


###  https://auroral.iot.linkeddata.es/def/core#NodeStatus
core:NodeStatus rdf:type owl:Class ;
                rdfs:subClassOf core:Status ;
                rdfs:label "Node status" .


###  https://auroral.iot.linkeddata.es/def/core#Notification
core:Notification rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Class ;
                                    owl:unionOf ( core:Organisation
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty core:hasOwner ;
                                                    owl:someValuesFrom core:User
                                                  ]
                                                )
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasStatus ;
                                    owl:allValuesFrom core:NotificationStatus
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasActor ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasKey ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasObject ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasTarget ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasCreationDate ;
                                    owl:allValuesFrom xsd:dateTime
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:isUnread ;
                                    owl:allValuesFrom xsd:boolean
                                  ] ;
                  rdfs:label "Notification" .


###  https://auroral.iot.linkeddata.es/def/core#NotificationStatus
core:NotificationStatus rdf:type owl:Class ;
                        rdfs:subClassOf core:Status ;
                        rdfs:label "Notification status" .


###  https://auroral.iot.linkeddata.es/def/core#Organisation
core:Organisation rdf:type owl:Class ;
                  rdfs:subClassOf org:Organization ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty geo:location ;
                                    owl:allValuesFrom geo:SpatialThing
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasAudit ;
                                    owl:allValuesFrom core:Audit
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasNode ;
                                    owl:allValuesFrom core:Node
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasNotification ;
                                    owl:allValuesFrom core:Notification
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasUser ;
                                    owl:allValuesFrom core:User
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:isRepresentedByAvatar ;
                                    owl:allValuesFrom foaf:Image
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasCreationDate ;
                                    owl:someValuesFrom xsd:dateTime
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasID ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasLocation ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasName ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasRegistrationNumber ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:hasSkinColor ;
                                    owl:someValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty core:isLastUpdated ;
                                    owl:someValuesFrom xsd:dateTime
                                  ] ;
                  rdfs:label "Organisation" .


###  https://auroral.iot.linkeddata.es/def/core#PhysicalThing
core:PhysicalThing rdf:type owl:Class ;
                   rdfs:subClassOf core:Item ;
                   rdfs:label "Physical Thing" .


###  https://auroral.iot.linkeddata.es/def/core#Place
core:Place rdf:type owl:Class ;
           rdfs:label "Place" .


###  https://auroral.iot.linkeddata.es/def/core#Role
core:Role rdf:type owl:Class ;
          rdfs:label "Role" .


###  https://auroral.iot.linkeddata.es/def/core#Sensor
core:Sensor rdf:type owl:Class ;
            rdfs:subClassOf <http://www.w3.org/ns/sosa/Sensor> ,
                            core:Device ;
            rdfs:label "Sensor" .


###  https://auroral.iot.linkeddata.es/def/core#Service
core:Service rdf:type owl:Class ;
             rdfs:subClassOf core:VirtualThing ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://lexvo.org/ontology#language> ;
                               owl:someValuesFrom <http://lexvo.org/ontology#Language>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:applicableToGeographicalArea ;
                               owl:someValuesFrom core:Place
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:hasInput ;
                               owl:someValuesFrom <http://www.w3.org/ns/ssn/Property>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:hasDomain ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:hasFunctionality ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:hasRequirement ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:hasSubDomain ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:serviceDescription ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty core:serviceFree ;
                               owl:someValuesFrom xsd:boolean
                             ] ,
