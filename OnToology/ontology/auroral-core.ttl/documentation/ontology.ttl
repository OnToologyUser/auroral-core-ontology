                                   rdfs:label "Person" .


###  https://auroral.iot.linkeddata.es/def/core#Actuator
:Actuator rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/sosa/Actuator> ,
                          :Device ;
          rdfs:label "Actuator" .


###  https://auroral.iot.linkeddata.es/def/core#AmbientProperty
:AmbientProperty rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                 rdfs:label "Ambient Property" .


###  https://auroral.iot.linkeddata.es/def/core#Audit
:Audit rdf:type owl:Class ;
       rdfs:label "Audit" .


###  https://auroral.iot.linkeddata.es/def/core#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf :Item ,
                        <https://saref.etsi.org/core#Device> ;
        rdfs:label "Device" .


###  https://auroral.iot.linkeddata.es/def/core#EnergyMonitor
:EnergyMonitor rdf:type owl:Class ;
               rdfs:subClassOf :Sensor ;
               rdfs:label "Energy Monitor" .


###  https://auroral.iot.linkeddata.es/def/core#EnergyProperty
:EnergyProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:label "Energy Property" .


###  https://auroral.iot.linkeddata.es/def/core#FloodSensor
:FloodSensor rdf:type owl:Class ;
             rdfs:subClassOf :Sensor ;
             rdfs:label "Flood Sensor" .


###  https://auroral.iot.linkeddata.es/def/core#GPSEmergencyButton
:GPSEmergencyButton rdf:type owl:Class ;
                    rdfs:subClassOf :Sensor ;
                    rdfs:label "G P S Emergency Button" .


###  https://auroral.iot.linkeddata.es/def/core#HumiditySensor
:HumiditySensor rdf:type owl:Class ;
                rdfs:subClassOf :Sensor ;
                rdfs:label "Humidity Sensor" .


###  https://auroral.iot.linkeddata.es/def/core#Item
:Item rdf:type owl:Class ;
      rdfs:subClassOf <https://www.w3.org/2019/wot/td#Thing> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasStatus ;
                        owl:someValuesFrom :ItemStatus
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCreationDate ;
                        owl:allValuesFrom xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasName ;
                        owl:allValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasUpdateDate ;
                        owl:allValuesFrom xsd:dateTime
                      ] ;
      rdfs:label "Item" .


###  https://auroral.iot.linkeddata.es/def/core#ItemStatus
:ItemStatus rdf:type owl:Class ;
            rdfs:subClassOf :Status ;
            rdfs:label "Item status" .


###  https://auroral.iot.linkeddata.es/def/core#LightProperty
:LightProperty rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
               rdfs:label "Light Property" .


###  https://auroral.iot.linkeddata.es/def/core#LuminusFlux
:LuminusFlux rdf:type owl:Class ;
             rdfs:subClassOf :LightProperty ;
             rdfs:label "Luminus Flux" .


###  https://auroral.iot.linkeddata.es/def/core#MonitoringSensor
:MonitoringSensor rdf:type owl:Class ;
                  rdfs:subClassOf :Sensor ;
                  rdfs:label "Monitoring Sensor" .


###  https://auroral.iot.linkeddata.es/def/core#MotionProperty
:MotionProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:label "Motion Property" .


###  https://auroral.iot.linkeddata.es/def/core#MotionSensor
:MotionSensor rdf:type owl:Class ;
              rdfs:subClassOf :Sensor ;
              rdfs:label "Motion Sensor" .


###  https://auroral.iot.linkeddata.es/def/core#Node
:Node rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :containsItem ;
                        owl:allValuesFrom :Item
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasStatus ;
                        owl:allValuesFrom :NodeStatus
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCreationDate ;
                        owl:someValuesFrom xsd:dateTime
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasID ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasKey ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasLocation ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasName ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasToken ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:label "Node" .


###  https://auroral.iot.linkeddata.es/def/core#NodeStatus
:NodeStatus rdf:type owl:Class ;
            rdfs:subClassOf :Status ;
            rdfs:label "Node status" .


###  https://auroral.iot.linkeddata.es/def/core#Notification
:Notification rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Class ;
                                owl:unionOf ( :Organisation
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasOwner ;
                                                owl:someValuesFrom :User
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasStatus ;
                                owl:allValuesFrom :NotificationStatus
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasActor ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasKey ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasObject ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasTarget ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasCreationDate ;
                                owl:allValuesFrom xsd:dateTime
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isUnread ;
                                owl:allValuesFrom xsd:boolean
                              ] ;
              rdfs:label "Notification" .


###  https://auroral.iot.linkeddata.es/def/core#NotificationStatus
:NotificationStatus rdf:type owl:Class ;
                    rdfs:subClassOf :Status ;
                    rdfs:label "Notification status" .


###  https://auroral.iot.linkeddata.es/def/core#Organisation
:Organisation rdf:type owl:Class ;
              rdfs:subClassOf <https://www.w3.org/ns/org#Organization> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
                                owl:allValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasAudit ;
                                owl:allValuesFrom :Audit
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasNode ;
                                owl:allValuesFrom :Node
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasNotification ;
                                owl:allValuesFrom :Notification
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasUser ;
                                owl:allValuesFrom :User
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isRepresentedByAvatar ;
                                owl:allValuesFrom <http://xmlns.com/foaf/0.1/Image>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasCreationDate ;
                                owl:someValuesFrom xsd:dateTime
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasID ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasLocation ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasName ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasRegistrationNumber ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasSkinColor ;
                                owl:someValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isLastUpdated ;
                                owl:someValuesFrom xsd:dateTime
                              ] ;
              rdfs:label "Organisation" .


###  https://auroral.iot.linkeddata.es/def/core#Role
:Role rdf:type owl:Class ;
      rdfs:label "Role" .


